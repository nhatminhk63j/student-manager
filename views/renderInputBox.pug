
extends base

block content 
    .finding-box
        form.input-box(method='POST')
            .form-group
                input.form-control.m-3(id='txt1-search' type='text' name='year' placeholder='Enter year')
                input.form-control.m-3(id='txt2-search' type='text' name='semester' placeholder='Enter semester')
                input.btn.btn-primary.btn-block.m-3(id='img-search' type='submit' value='Search')
        